{
    "name": "digital-creative/column-toggler",
    "description": "A Laravel Nova package that allows you to hide/show columns in the index view.",
    "keywords": [
        "laravel",
        "nova",
        "column-toggle"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\ColumnToggler\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\ColumnToggler\\ColumnTogglerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

